def is_leap(year):
    
    # Write your logic here
    
    if (year<2000):
        if (year%4==0):
            return True
        else:
                return False
    else:
        if (year%100==0 and year%400==0):
            return True
        else:
                return False

year = int(input())
print(is_leap(year))
